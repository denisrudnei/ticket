{
  "name": "ticket-dns",
  "version": "1.0.0",
  "description": "Ticket management system",
  "author": "Denis Rudnei de Souza",
  "private": true,
  "engines": {
    "node": ">=13.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register server/index.ts --transpileOnly --respawn",
    "dev:client": "nuxt dev --port 4000",
    "seed": "cross-env NODE_ENV=development nodemon --watch test/ --exec  \"ts-node -r tsconfig-paths/register test/seedDb.ts\" -e ts",
    "backend": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register server/onlyServer.ts",
    "build-server": "tsc -p tsconfig-build.json",
    "tsc": "tsc -p tsconfig-build.json",
    "start": "nuxt start",
    "start-server": "cross-env NODE_ENV=production node -r tsconfig-paths/register dist/server/index.js",
    "heroku-postbuild": "npm run build-server",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore . --fix",
    "precommit": "npm run lint",
    "test": "nyc --reporter=html --reporter=text mocha -r tsconfig-paths/register test/** --file test/setup.ts --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^23.1.0",
    "@ckeditor/ckeditor5-vue": "^1.0.3",
