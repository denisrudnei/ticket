{
  "name": "ticket-dns",
  "version": "1.0.0",
  "description": "Ticket management system",
  "author": "Denis Rudnei de Souza",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch server/ --exec  \"ts-node -r tsconfig-paths/register server/index.ts\" -e ts",
    "backend": "cross-env NODE_ENV=development nodemon --watch server/ --exec  \"ts-node -r tsconfig-paths/register server/onlyServer.ts\" -e ts",
    "build": "nuxt build && tsc -p tsconfig-build.json",
    "tsc": "tsc -p tsconfig-build.json",
    "start": "cross-env NODE_ENV=production node -r tsconfig-paths/register dist/server/index.js",
    "heroku-postbuild": "npm run build",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore . --fix",
    "precommit": "npm run lint",
    "test": "nyc --reporter=html --reporter=text mocha -r tsconfig-paths/register test/** --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^12.4.0",
    "@ckeditor/ckeditor5-vue": "^1.0.1",
